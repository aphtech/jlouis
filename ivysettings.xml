<ivysettings>
  <settings defaultResolver="chain1"/>
  <resolvers>
    <url name="javanet" m2compatible="true">
      <artifact pattern="http://download.java.net/maven/2/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
    </url>
    <ibiblio name="main_ibiblio" m2compatible="true" />
    <chain name="chain1">
      <resolver ref="javanet" />
      <resolver ref="main_ibiblio" />
    </chain>
  </resolvers>
</ivysettings>

